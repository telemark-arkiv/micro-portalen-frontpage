{
  "name": "micro-portalen-frontpage",
  "description": "Service for checking if Portalen is live",
  "version": "1.3.0",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=8.8.1"
  },
  "scripts": {
    "test": "standard && nsp check && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "start": "micro",
    "dev": "micro-dev",
    "now-deploy": "npm test && now --npm && now alias"
  },
  "keywords": [
    "microservice",
    "micro",
    "roles",
    "portalen",
    "Telemark fylkeskommune"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/telemark/micro-portalen-frontpage.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/micro-portalen-frontpage/issues"
  },
  "homepage": "https://github.com/telemark/micro-portalen-frontpage#readme",
  "now": {
    "alias": [
      "start.portalen.win"
    ],
    "env": {
      "NODE_ENV": "production"
    },
    "type": "npm"
  },
  "devDependencies": {
    "coveralls": "3.0.1",
    "ava": "0.25.0",
    "nsp": "3.2.1",
    "micro-dev": "3.0.0",
    "nyc": "12.0.1",
    "standard": "11.0.1",
    "test-listen": "1.1.0"
  },
  "dependencies": {
    "axios": "0.18.0",
    "markdown-it": "8.4.1",
    "micro": "9.3.2"
  }
}
